{
	"name": "jetstreamlabs/generators",
	"description": "Generators package for Jetlabs application framework.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "secondman",
			"email": "secondman@jetstreamlabs.com"
		}
	],
	"require": {
		"php": "^8.1",
		"illuminate/console": "^8.0",
		"illuminate/database": "^8.0",
		"illuminate/support": "^8.0"
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Jetlabs\\Generators\\Providers\\ConsoleSupportServiceProvider"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"Jetlabs\\Generators\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"styleci/cli": "^1.2"
	}
}
